  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,md}": [
      "prettier --write"
    ],
    "*.ts?(x)": "tsc-files --noEmit"
  },
  "prettier": "@totejs/prettier-config",
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals"
    ],
    "rules": {
      "react/display-name": "off",
      "react/react-in-jsx-scope": [
        "off"
      ],
      "react/no-unknown-property": [
        "off"
      ],
      "react/no-unescaped-entities": [
        "off"
      ]
    }
  }
